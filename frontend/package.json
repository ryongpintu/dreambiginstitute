{
    "name": "dream-big-portal",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next build && next export",
        "format": "prettier --ignore-path .prettierignore --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "clean": "rimraf *.tsbuildinfo .next out yarn-error.log",
        "clean-all": "rimraf *.tsbuildinfo node_modules .husky .next out yarn-error.log",
        "typecheck": "tsc -p tsconfig.json",
        "lint": "next lint",
        "lint:fix": "yarn typecheck & yarn format & yarn lint --fix",
        "prepare": "cd .. && husky install Frontend/.husky",
        "postbuild": "next-sitemap"
    },
    "dependencies": {
        "@googlemaps/react-wrapper": "^1.1.35",
        "@googlemaps/typescript-guards": "^2.0.3",
        "axios": "^1.6.8",
        "clsx": "^1.2.1",
        "dayjs": "^1.11.6",
        "disqus-react": "^1.1.3",
        "fast-equals": "^4.0.3",
        "framer-motion": "^7.6.7",
        "gray-matter": "^4.0.3",
        "loading-attribute-polyfill": "^2.1.0",
        "marked": "^4.2.2",
        "next": "^13.0.3",
        "next-pwa": "^5.6.0",
        "next-react-svg": "https://github.com/hasanm95/next-react-svg",
        "next-seo": "^5.14.1",
        "next-sitemap": "^3.1.32",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.39.4",
        "react-hot-toast": "^2.4.1",
